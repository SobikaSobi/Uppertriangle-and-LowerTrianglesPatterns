class HelloWorld {
    public static void main(String[] args) {
        for(int i=1;i<=5;i++){
           // for(int j=1;j<=i;j++){ for increasing trianglr
            for(int j=5;j>=i;j--){
                 System.out.print("*");
            }
          System.out.println();   }
        
       
    }
}
